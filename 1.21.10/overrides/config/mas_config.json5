	"MaceShockwaveSize": 1.0,
	// Sets the opacity of the mace shockwave
	"MaceShockwaveOpacity": 1.0,
	// Creates spark particles when slamming with the mace
	"MaceSpark": true,
	// Creates flash upon slamming with the mace
	"MaceFlash": true,
	// Creates a small shockwave on a playerÂ´s shield when blocking mace attacks
	"ShieldEffect": true,
	// If enabled spawns armor breaking particles when using the breach enchantment
	"ArmorParticles": true,
	// Spawns particles when a wind charge explodes
	"WindParticles": true,
	// If enabled changes the totem particle effect completely
	"CustomTotemEffect": true,
